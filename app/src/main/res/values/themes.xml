<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AlertoSanNicolas" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#286cab</item>
        <item name="colorPrimaryVariant">#286cab</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#6793be</item>
        <item name="colorSecondaryVariant">#6793be</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#286cab</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AlertoSanNicolas.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AlertoSanNicolas.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AlertoSanNicolas.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>